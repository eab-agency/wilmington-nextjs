{
  "name": "wilmington-website",
  "version": "0.1.0",
  "engines": {
    "node": ">=20"
  },
  "private": true,
  "scripts": {
    "build": "faust build",
    "clean": "rimraf .next/",
    "dev": "NODE_OPTIONS='-r newrelic' npm run clean && faust dev",
    "develop": "faust dev",
    "format": "npx prettier  '**/*.{js,jsx,ts,tsx,md,html,css,scss,json,yml}' --write || true",
    "generate": "faust generatePossibleTypes",
    "import-redirects": "node scripts/import-redirects.js",
    "lint": "npm run format && npm run lint:js && npm run lint:css",
    "lint:css": "npx stylelint '**/*.css' --fix || true",
    "lint:js": "npx eslint '**/*.{js,ts,jsx,tsx}' --fix || true && next lint",
    "postbuild": "next-sitemap",
    "prepare": "husky install",
    "start": "NODE_OPTIONS='-r newrelic' faust start"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@faustwp/blocks": "^4.0.0",
    "@faustwp/cli": "3.2.3",
    "@faustwp/core": "^3.2.3",
    "@microsoft/clarity": "^1.0.0",
    "@mui/material": "^6.4.2",
    "@newrelic/next": "^0.10.0",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vercel/analytics": "^1.0.1",
    "@wordpress/base-styles": "^4.26.0",
    "@wordpress/block-editor": "^14.18.0",
    "@wordpress/block-library": "^9.23.0",
    "@wordpress/components": "^29.9.0",
    "@wordpress/core-data": "^7.23.0",
    "@wordpress/style-engine": "^1.38.0",
    "algoliasearch": "^4.16.0",
    "apollo-link-rest": "^0.9.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "deepmerge": "^4.3.1",
    "dotenv": "^17.2.1",
    "formik": "^2.4.6",
    "glob": "^10.2.2",
    "graphql": "^16.11.0",
    "html-react-parser": "^5.2.5",
    "isomorphic-unfetch": "^4.0.2",
    "lodash": "^4.17.21",
    "newrelic": "^12.18.2",
    "next": "^15.3.3",
    "next-auth": "^4.24.5",
    "next-sitemap": "^4.0.7",
    "postcss": "^8.5.3",
    "qs": "^6.11.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-gtm-module": "^2.0.11",
    "react-html-parser": "^2.0.2",
    "react-icons": "^4.8.0",
    "react-instantsearch": "^7.15.7",
    "react-multi-carousel": "^2.8.6",
    "react-svg": "^16.1.32",
    "react-tabs": "^6.0.1",
    "rimraf": "^5.0.0",
    "sass": "^1.88.0",
    "sst": "3.14.25",
    "typescript": "5.8.3",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@types/newrelic": "^9.14.8",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^8.38.0",
    "eslint-config-next": "^15.3.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^7.0.2",
    "prettier": "^2.8.7",
    "sharp": "^0.33.3",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-scss": "^4.6.0"
  }
}
